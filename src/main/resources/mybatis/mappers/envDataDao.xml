<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.saeromteo.app.Dao">

    <select id="readAll" resultType="com.saeromteo.app.dto.EnvironmentDataDto.Response">
    	SELECT * FROM ENVIRONMENTDATA;
    </select>
    <select id="readDetail" parameterType="int" resultType="com.saeromteo.app.dto.EnvironmentDataDto.Response">
    	SELECT * FROM ENVIRONMENTDATA WHERE ENV_ID=#{ID}
    </select>
    <select id="readByCategory" parameterType="String" resultType="com.saeromteo.app.dto.EnvironmentDataDto.Response">
    	SELECT * FROM ENVIRONMENTDATA WHERE ENV_CATEGORY=#{CATEGORY}
    </select>
    <select id="readByType" parameterType="String" resultType="com.saeromteo.app.dto.EnvironmentDataDto.Response">
    	SELECT * FROM ENVIRONMENTDATA WHERE ENV_TYPE=#{TYPE}
    </select>
    
    
    

</mapper>
