<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.searomteo.app.notice">

	<!-- Read -->
	<select id="searchAll" resultType="questionresponsevo">
		SELECT * FROM QUESTIONS
	</select>
	
	<select id="seachCategory" parameterType="string" resultType="questionresponsevo">
		SELECT * FROM QUESTIONS
		WHERE QUESTION_CATEGORY = #{questionCategory}
	</select>
	
	<select id="searchUser" parameterType="int" resultType="questionresponsevo">
		SELECT * FROM QUESTIONS
		WHERE USER_CODE = #{userCode}
	</select>
	
	<select id="searchDetail" parameterType="int" resultType="questionresponsevo">
		SELECT * FROM QUESTIONS
		WHERE QUESTION_ID = #{questionId}
	</select>
	
	<!-- Create -->
	<insert id=""></insert>
</mapper>